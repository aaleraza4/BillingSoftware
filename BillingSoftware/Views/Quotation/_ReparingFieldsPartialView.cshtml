@model IEnumerable<Billing.DTOs.DTOs.SparePartFieldDTO>
@if (Model != null && Model.Count() > 0)
{
    foreach (var SparePart in Model)
    {
        <div class="form-row SpartPart-@SparePart.SparePartId">
            <div class="form-group col-md-6">
                <label asp-for="@SparePart.SparePartName">Name</label>
                <input type="text" class="form-control" data-SparePartId="@SparePart.SparePartId" asp-for="@SparePart.SparePartName" placeholder="Enter Spare Part">
            </div>
            <div class="form-group col-md-6">
                <label asp-for="@SparePart.SparePartQuantity">Quantity</label>
                <input type="text" class="form-control" data-SparePartId="@SparePart.SparePartId" asp-for="@SparePart.SparePartQuantity" placeholder="Enter Quantity">
            </div>
        </div>
    }
}